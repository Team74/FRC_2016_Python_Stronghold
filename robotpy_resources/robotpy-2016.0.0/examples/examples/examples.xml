  <tagDescription>
    <name>CommandBased Robot</name>
    <description>Examples for CommandBased robot programs.</description>
  </tagDescription>

  <example>
    <name>GearsBot</name>
    <description>A fully functional example CommandBased program for WPIs GearsBot robot. This code can run on your computer if it supports simulation.</description>
    <tags>
      <tag>CommandBased Robot</tag>
    </tags>
    <world>/usr/share/frcsim/worlds/GearsBotDemo.world</world>
    <packages>
      <package>src</package>
      <package>src/commands</package>
      <package>src/subsystems</package>
    </packages>
    <files>
      <file source="examples/GearsBot/OI.py" destination="src/OI.py"></file>
      <file source="examples/GearsBot/robot.py" destination="src/robot.py"></file>
      <file source="examples/GearsBot/commands/__init__.py" destination="src/commands/__init__.py"></file>
      <file source="examples/GearsBot/commands/autonomous.py" destination="src/commands/autonomous.py"></file>
      <file source="examples/GearsBot/commands/close_claw.py" destination="src/commands/close_claw.py"></file>
      <file source="examples/GearsBot/commands/drive_straight.py" destination="src/commands/drive_straight.py"></file>
      <file source="examples/GearsBot/commands/open_claw.py" destination="src/commands/open_claw.py"></file>
      <file source="examples/GearsBot/commands/pickup.py" destination="src/commands/pickup.py"></file>
      <file source="examples/GearsBot/commands/place.py" destination="src/commands/place.py"></file>
      <file source="examples/GearsBot/commands/prepare_to_pickup.py" destination="src/commands/prepare_to_pickup.py"></file>
      <file source="examples/GearsBot/commands/set_distance_to_box.py" destination="src/commands/set_distance_to_box.py"></file>
      <file source="examples/GearsBot/commands/set_elevator_setpoint.py" destination="src/commands/set_elevator_setpoint.py"></file>
      <file source="examples/GearsBot/commands/set_wrist_setpoint.py" destination="src/commands/set_wrist_setpoint.py"></file>
      <file source="examples/GearsBot/commands/tank_drive_with_joystick.py" destination="src/commands/tank_drive_with_joystick.py"></file>
      <file source="examples/GearsBot/subsystems/__init__.py" destination="src/subsystems/__init__.py"></file>
      <file source="examples/GearsBot/subsystems/claw.py" destination="src/subsystems/claw.py"></file>
      <file source="examples/GearsBot/subsystems/drivetrain.py" destination="src/subsystems/drivetrain.py"></file>
      <file source="examples/GearsBot/subsystems/elevator.py" destination="src/subsystems/elevator.py"></file>
      <file source="examples/GearsBot/subsystems/wrist.py" destination="src/subsystems/wrist.py"></file>
    </files>
  </example>

  <example>
    <name>PacGoat</name>
    <description>A fully functional example CommandBased program for FRC Team 190&#39;s 2014 robot. This code can run on your computer if it supports simulation.</description>
    <tags>
      <tag>CommandBased Robot</tag>
    </tags>
    <world>/usr/share/frcsim/worlds/PacGoat2014.world</world>
    <packages>
      <package>src</package>
      <package>src/commands</package>
      <package>src/subsystems</package>
      <package>src/triggers</package>
    </packages>
    <files>
      <file source="examples/pacgoat/oi.py"
            destination="src/oi.py"></file>
      <file source="examples/pacgoat/robot.py"
            destination="src/robot.py"></file>
      <file source="examples/pacgoat/commands/__init__.py"
            destination="src/commands/__init__.py"></file>
      <file source="examples/pacgoat/commands/check_for_hot_goal.py"
            destination="src/commands/check_for_hot_goal.py"></file>
      <file source="examples/pacgoat/commands/close_claw.py"
            destination="src/commands/close_claw.py"></file>
      <file source="examples/pacgoat/commands/collect.py"
            destination="src/commands/collect.py"></file>
      <file source="examples/pacgoat/commands/drive_and_shoot_autonomous.py"
            destination="src/commands/drive_and_shoot_autonomous.py"></file>
      <file source="examples/pacgoat/commands/drive_forward.py"
            destination="src/commands/drive_forward.py"></file>
      <file source="examples/pacgoat/commands/drive_with_joystick.py"
            destination="src/commands/drive_with_joystick.py"></file>
      <file source="examples/pacgoat/commands/extend_shooter.py"
            destination="src/commands/extend_shooter.py"></file>
      <file source="examples/pacgoat/commands/low_goal.py"
            destination="src/commands/low_goal.py"></file>
      <file source="examples/pacgoat/commands/open_claw.py"
            destination="src/commands/open_claw.py"></file>
      <file source="examples/pacgoat/commands/set_collection_speed.py"
            destination="src/commands/set_collection_speed.py"></file>
      <file source="examples/pacgoat/commands/set_pivot_setpoint.py"
            destination="src/commands/set_pivot_setpoint.py"></file>
      <file source="examples/pacgoat/commands/shoot.py"
            destination="src/commands/shoot.py"></file>
      <file source="examples/pacgoat/commands/wait_for_ball.py"
