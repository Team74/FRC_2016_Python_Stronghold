      <file source="examples/pacgoat/subsystems/collector.py"
            destination="src/subsystems/collector.py"></file>
      <file source="examples/pacgoat/subsystems/drivetrain.py"
            destination="src/subsystems/drivetrain.py"></file>
      <file source="examples/pacgoat/subsystems/pivot.py"
            destination="src/subsystems/pivot.py"></file>
      <file source="examples/pacgoat/subsystems/pneumatics.py"
            destination="src/subsystems/pneumatics.py"></file>
      <file source="examples/pacgoat/subsystems/shooter.py"
            destination="src/subsystems/shooter.py"></file>
      <file source="examples/pacgoat/triggers/__init__.py"
            destination="src/triggers/__init__.py"></file>
      <file source="examples/pacgoat/triggers/double_button.py"
            destination="src/triggers/double_button.py"></file>
    </files>
  </example>

